@RestResource
public interface ClientRepo extends JpaRepository<Client, Long> {

    @Query(value = "SELECT c.name c from client c Where c.name like '%{#name}%'", nativeQuery = true)
    List<Client> search_by_name(@PathParam("id") String id, @PathParam("name") String name);

    @Query(value = "SELECT V FROM users_ventes U_V,commande C, ventes V Where U_V.users_id={#id}and V.ventes_id=U_V.id,v.numero_commande={#name} ", nativeQuery = true)
    List<ventes> getVenteCommande(@PathParam("id") Long id, @PathParam("name") String name);


