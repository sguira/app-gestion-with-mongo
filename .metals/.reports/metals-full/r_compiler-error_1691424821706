    static int search_category(List<Categorie> list, String name) {

        if (list.size() > 0) {
            for (int i = 0; i < list.size(); i++) {
                if (list.get(i).getName() != null) {
                    if (list.get(i).getName().equals(name)) {
                        return 1;
                    }
                }

            }
        }
        return 0;
    }
